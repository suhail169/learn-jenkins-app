{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "LearnJenkinsApp-TaskDefinition-Prod",
    "containerDefinitions": [
        {
            "name": "learnjenkinsapp",
            "image": "156427154026.dkr.ecr.ap-south-1.amazonaws.com/learnjenkinsapp:#APP_VERSION#",
            "portMappings": [
                {
                    "name": "ngnix-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::156427154026:role/ecsTaskExecutionRole"
}
